# program defaults

#setenv CVS_RSH   "ssh"
#setenv CVSROOT   ":pserver:macdonre@localhost:/cvs/root"
setenv FTPPROMPT  "%n@%m:%c3%% "
setenv FTPRPROMPT "ftp"
setenv MYSQL_PS1  "\u@\h:\d> "
setenv EDITOR     "emacs"
#setenv PAGER     "more"
setenv PATH       "/opt/local/bin:/usr/local/bin:/usr/local/sbin"
setenv PATH       "${PATH}:${HOME}/Library/Bin:/usr/bin:/usr/sbin:/bin:/sbin"
setenv ANT_OPTS   "-Xmx256m -Xss4096k"
setenv MANPATH    "/opt/local/man:/usr/local/man:${MANPATH}"

# Java
setenv JAVA_HOME      /System/Library/Frameworks/JavaVM.framework/Home
setenv JAVA_VERSIONS  /System/Library/Frameworks/JavaVM.framework/Versions
setenv JAVA_CURRENT   ${JAVA_VERSIONS}/CurrentJDK
setenv JAVA_1_2       ${JAVA_VERSIONS}/1.2
setenv JAVA_1_3       ${JAVA_VERSIONS}/1.3
setenv JAVA_1_3_1     ${JAVA_VERSIONS}/1.3.1
setenv JAVA_1_4_1     ${JAVA_VERSIONS}/1.4.1
setenv JAVA_1_4_2     ${JAVA_VERSIONS}/1.4.2
setenv JAVA_1_5       ${JAVA_VERSIONS}/1.5
setenv JAVA_1_5_0     ${JAVA_VERSIONS}/1.5.0

# Ruby
setenv RUBY_HOME      /usr/lib/ruby/1.8/
setenv GEMS_HOME      /Library/Ruby/Gems/1.8/gems/
setenv GEMS_HOME_SYS  /usr/lib/ruby/gems/1.8/gems/

# path shortcuts
setenv WORK     ~/Work
setenv CODE     ${WORK}/YellowPages
setenv ELNK     ${WORK}/EarthLink
setenv LAT      ${WORK}/LATimes
setenv GITHUB   ${WORK}/GitHub

# mac os x
setenv TRASH      ~/.Trash
setenv DESKTOP    ~/Desktop
setenv DOWNLOADS  ~/Downloads

# irc
setenv IRCNICK    reidmix
setenv IRCNAME    reidmix@gmail.com

